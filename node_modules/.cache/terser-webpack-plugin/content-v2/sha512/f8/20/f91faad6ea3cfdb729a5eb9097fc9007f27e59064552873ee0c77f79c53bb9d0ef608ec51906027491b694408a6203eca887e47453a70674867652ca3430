\",[t._v(\"初次渲染过程：\\n\"),a(\"ul\",[a(\"li\",[t._v(\"第一template解析为render函数(或者是开发环境已经完成，vue-loader，拿到的就是成品的rendr函数)\")]),t._v(\" \"),a(\"li\",[t._v(\"第二触发响应式，监听data属性,geter setter\")]),t._v(\" \"),a(\"li\",[t._v(\"第三,执行render函数生成vnode ,执行patch(elem,vnode);\\n\"),a(\"ul\",[a(\"li\",[t._v(\"模板内没有用到的data属性的改变不会触发渲染，因为和视图无关\")])])])])]),t._v(\" \"),a(\"li\",[t._v(\"更新过程：\\n\"),a(\"ul\",[a(\"li\",[t._v(\"修改data，触发setter(此前getter里已经被监听)\")]),t._v(\" \"),a(\"li\",[t._v(\"重新直接render函数，生成newVnode\")]),t._v(\" \"),a(\"li\",[t._v(\"patch(vnode,newvnode)\\n\"),a(\"ul\",[a(\"li\",[t._v(\"patch的diff算法会算出最小差异\")])])])])]),t._v(\" \"),a(\"li\",[t._v(\"异步渲染\\n\"),a(\"ul\",[a(\"li\",[t._v(\"$nextTick\\n\"),a(\"ul\",[a(\"li\",[t._v(\"vue组件是异步渲染，nextTick等DOM渲染完进行回调，页面渲染时会把data数据整合，多次data修改只渲染一次\")])])]),t._v(\" \"),a(\"li\",[t._v(\"汇总data的修改，一次性渲染视图。\")]),t._v(\" \"),a(\"li\",[t._v(\"减少DOM操作次数，提高性能。\")])])])]),t._v(\" \"),a(\"h2\",{attrs:{id:\"vue3为什么使用proxy\"}},[a(\"a\",{staticClass:\"header-anchor\",attrs:{href:\"#vue3为什么使用proxy\"}},[t._v(\"#\")]),t._v(\" vue3为什么使用proxy\")]),t._v(\" \"),a(\"p\",[t._v(\"https://www.jb51.net/article/171869.htm\")])])}),[],!1,null,null,null);s.default=e.exports}}]);","extractedComments":[]}